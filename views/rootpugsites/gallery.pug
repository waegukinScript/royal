            span.icon-bar
            span.icon-bar
          // Collect the nav links, forms, and other content for toggling
          #navbarSupportedContent.collapse.navbar-collapse.offset
            ul.nav.navbar-nav.menu_nav.ml-auto
              li.nav-item
                a.nav-link(href='index.html') Home
              li.nav-item
                a.nav-link(href='about.html') About us
              li.nav-item
                a.nav-link(href='accomodation.html') Accomodation
              li.nav-item.active
                a.nav-link(href='gallery.html') Gallery
              li.nav-item.submenu.dropdown
                a.nav-link.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') Blog
